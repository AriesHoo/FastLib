<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">#FFFFFF</color>
    <color name="colorPrimaryDark">#FFFFFF</color>
    <color name="colorAccent">#393939</color>
    <!--不设置style里
    <item name="colorAccent">@color/colorAccent</item>
    属性指向可单独设置改颜色用于达到相同效果-->
    <!--<color name="accent_material_light">#b640ff</color>-->

    <!--普通有灰色背景的用这个-->
    <color name="colorBackground">#f8f8f8</color>
    <!--滑动返回windowBackground-->
    <color name="colorSwipeBackBackground">#0a000000</color>

    <color name="colorMainAli">#1b81d1</color>
    <color name="colorMainNews">#2B61BF</color>

    <color name="colorQQBackgroundStart">#4e8cfc</color>
    <color name="colorQQBackgroundEnd">#33bafc</color>
    <color name="colorQQBackgroundCenter">#34b0fc</color>

    <!--以下属性为lib属性app里设置可以影响lib默认-->

    <!--TitleBarView背景色-->
    <color name="colorTitleBackground">#ffffff</color>
    <!--TitleBarView 文字颜色-->
    <color name="colorTitleText">#393939</color>
    <!--TitleBarView 下划线颜色-->
    <color name="colorTitleDivider">#DCDCDC</color>

    <!--主页tab背景色-->
    <color name="colorTabBackground">#FCFCFC</color>
    <!--主页上划线颜色-->
    <color name="colorTabUnderline">#d6d6d6</color>
    <!--主页tab文字默认颜色-->
    <color name="colorTabTextUnSelect">#909090</color>
    <!--主页tab文字选中颜色-->
    <color name="colorTabTextSelect">#393939</color>

    <!--多状态 loading颜色-->
    <color name="colorMultiProgress">@color/colorTitleText</color>
    <!--多状态文字颜色-->
    <color name="colorMultiText">@color/colorTitleText</color>
    <!--多状态布局背景色-->
    <color name="colorMultiBackground">#fafafa</color>
    <!--多状态布局图片颜色-->
    <color name="colorMultiImage">@color/colorTitleText</color>

    <!--Glide加载图片默认占位图背景色-->
    <color name="colorPlaceholder">#bdbdbd</color>

    <!--LoadMoreView 文本颜色-->
    <color name="colorLoadMoreText">@color/colorTitleText</color>
    <!--LoadMoreView loading颜色-->
    <color name="colorLoadMoreProgress">@color/colorTitleText</color>
</resources>
